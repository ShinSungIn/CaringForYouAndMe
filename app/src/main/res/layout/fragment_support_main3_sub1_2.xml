<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		app:layout_behavior="@string/appbar_scrolling_view_behavior"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".SupportThiActivity">

	<LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:orientation="vertical">


		<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/text_margin"
				android:layout_marginLeft="@dimen/_8dp"
				android:layout_marginRight="@dimen/_8dp"
				android:textColor="@color/colorText_Black"
				android:textSize="@dimen/textView_fontsize"
				android:lineSpacingExtra="@dimen/textView_lineheight"
				android:text="1) 등록관리서비스"/>

		<android.support.v7.widget.AppCompatImageButton
				android:text="새창"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/support_main3_sub2_view1"
				android:src="@drawable/support_main3_sub2_1"
				android:adjustViewBounds="true"
				android:scaleType="fitCenter"
				android:background="@color/colorWhite"
				android:layout_margin="@dimen/text_margin"
		/>

		<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/text_margin"
				android:layout_marginLeft="@dimen/_8dp"
				android:layout_marginRight="@dimen/_8dp"
				android:textColor="@color/colorText_Black"
				android:textSize="@dimen/textView_fontsize"
				android:lineSpacingExtra="@dimen/textView_lineheight"
				android:text="2) 치매조기검진서비스"/>

		<android.support.v7.widget.AppCompatImageButton
				android:text="새창"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/support_main3_sub2_view2"
				android:src="@drawable/support_main3_sub2_2"
				android:adjustViewBounds="true"
				android:scaleType="fitCenter"
				android:background="@color/colorWhite"
				android:layout_margin="@dimen/text_margin"
		/>

		<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/text_margin"
				android:layout_marginLeft="@dimen/_8dp"
				android:layout_marginRight="@dimen/_8dp"
				android:textColor="@color/colorText_Black"
				android:textSize="@dimen/textView_fontsize"
				android:lineSpacingExtra="@dimen/textView_lineheight"
				android:text="3) 치매가족서비스"/>

		<ImageView android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:id="@+id/support_main3_sub2_view3"
				android:src="@drawable/support_main3_sub2_3"
				android:layout_marginLeft="@dimen/text_margin"
				android:layout_marginRight="@dimen/text_margin"
			   	android:layout_gravity="center_horizontal"
		/>

		<android.support.v7.widget.AppCompatImageButton
				android:text="새창"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/support_main3_sub2_view4"
				android:src="@drawable/support_main3_sub2_4"
				android:adjustViewBounds="true"
				android:scaleType="fitCenter"
				android:background="@color/colorWhite"
				android:layout_marginLeft="@dimen/text_margin"
				android:layout_marginRight="@dimen/text_margin"
		/>

		<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/text_margin"
				android:layout_marginLeft="@dimen/_8dp"
				android:layout_marginRight="@dimen/_8dp"
				android:textColor="@color/colorText_Black"
				android:textSize="@dimen/textView_fontsize"
				android:lineSpacingExtra="@dimen/textView_lineheight"
				android:text="4) 치매친화적 지역사회"/>

		<android.support.v7.widget.AppCompatImageButton
				android:text="새창"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/support_main3_sub2_view5"
				android:src="@drawable/support_main3_sub2_5"
				android:adjustViewBounds="true"
				android:scaleType="fitCenter"
				android:background="@color/colorWhite"
				android:layout_margin="@dimen/text_margin"
		/>


	</LinearLayout>

</android.support.v4.widget.NestedScrollView>
