<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		app:layout_behavior="@string/appbar_scrolling_view_behavior"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".CureprogramActivity">

	<LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:orientation="vertical">

		<ImageView android:layout_width="wrap_content" android:layout_height="wrap_content"
				   android:id="@+id/cure_main1_sub1_view14"
				   android:src="@drawable/cure_main1_sub1_14"
				   android:layout_marginTop="@dimen/text_margin"
				   android:layout_gravity="center_horizontal"
				   android:contentDescription="@string/cure_main1_sub14"
		/>

		<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="@dimen/text_margin"
				android:textColor="@color/colorText_Black"
				android:textSize="@dimen/textView_fontsize"
				android:lineSpacingExtra="@dimen/textView_lineheight"
				android:text="@string/text_cure_main1_sub14"/>

	</LinearLayout>

</android.support.v4.widget.NestedScrollView>
