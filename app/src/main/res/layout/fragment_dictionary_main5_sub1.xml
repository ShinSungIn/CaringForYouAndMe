<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		app:layout_behavior="@string/appbar_scrolling_view_behavior"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".DictionaryActivity">

	<LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:orientation="vertical">

		<Button android:text="위험한 물건에 접근 시"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/_8dp"
				android:layout_marginRight="@dimen/_8dp"
				android:layout_marginBottom="@dimen/_8dp"
				android:id="@+id/DicFivButton1"
				/>
		<Button android:text="집에 혼자 두는 경우"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/_8dp"
				android:layout_marginRight="@dimen/_8dp"
				android:layout_marginBottom="@dimen/_8dp"
				android:id="@+id/DicFivButton2"
		/>
		<Button android:text="공격적인 언어사용 시"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/_8dp"
				android:layout_marginRight="@dimen/_8dp"
				android:layout_marginBottom="@dimen/_8dp"
				android:id="@+id/DicFivButton3"
		/>
		<Button android:text="크고 빠르게 말할 시"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/_8dp"
				android:layout_marginRight="@dimen/_8dp"
				android:layout_marginBottom="@dimen/_8dp"
				android:id="@+id/DicFivButton4"
		/>


	</LinearLayout>

</android.support.v4.widget.NestedScrollView>